extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3.teal
                h3 Aluno recebido
            .w3-container
                h2
                    a(href="/lista") Ver lista de tarefas
                table.w3-table-all
                    tr
                        th Nome
                        td= aluno.nome
                    tr
                        th Identificador
                        td= aluno.numero
                    tr
                        th Curso
                        td= aluno.curso
            footer.w3-container.w3.teal
                address Gerado por FormApp

